Algoritmo "cdeta"

Var
    A, B, C, Delta: real

Inicio
    Escreva("Escreva um valor para A: ")
    Leia(A)

    Escreva("Escreva um valor para B: ")
    Leia(B)

    Escreva("Escreva um valor para C: ")
    Leia(C)

    Delta <- (B * B) - (4 * A * C)

    Escreval("O valor de Delta Ã© ", Delta)

Fimalgoritmo
